    "chalk": "^5.2.0",
    "csv-parse": "^5.4.0",
    "download": "^8.0.0",
    "lodash": "^4.17.21",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.5"
  }
}
